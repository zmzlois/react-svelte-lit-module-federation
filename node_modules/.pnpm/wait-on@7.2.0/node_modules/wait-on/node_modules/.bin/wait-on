#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lois/Documents/GitHub/react-svelte-lit-module-federation/node_modules/.pnpm/wait-on@7.2.0/node_modules/wait-on/bin/node_modules:/Users/lois/Documents/GitHub/react-svelte-lit-module-federation/node_modules/.pnpm/wait-on@7.2.0/node_modules/wait-on/node_modules:/Users/lois/Documents/GitHub/react-svelte-lit-module-federation/node_modules/.pnpm/wait-on@7.2.0/node_modules:/Users/lois/Documents/GitHub/react-svelte-lit-module-federation/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lois/Documents/GitHub/react-svelte-lit-module-federation/node_modules/.pnpm/wait-on@7.2.0/node_modules/wait-on/bin/node_modules:/Users/lois/Documents/GitHub/react-svelte-lit-module-federation/node_modules/.pnpm/wait-on@7.2.0/node_modules/wait-on/node_modules:/Users/lois/Documents/GitHub/react-svelte-lit-module-federation/node_modules/.pnpm/wait-on@7.2.0/node_modules:/Users/lois/Documents/GitHub/react-svelte-lit-module-federation/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/wait-on" "$@"
else
  exec node  "$basedir/../../bin/wait-on" "$@"
fi
